package Com.Sample;

//Parent class
class Person1 {
String name;
int age;

void showPerson() {
 System.out.println("Name: " + name);
 System.out.println("Age: " + age);
}
}

//Child class
class Employee1 extends Person1 {
int salary;

void showEmployee() {
 showPerson();
 System.out.println("Salary: " + salary);
}
}

class Manager1 extends Employee1 {
String pos;

void showManager() {
 showEmployee();
 System.out.println("Job Role: " + pos);
}
}

//Main class
public class MultiLevelinheritance {
	public static void main(String[] args) {
		// TODO Auto-generated method stub
 Manager1 m = new Manager1();

 m.name = "Tanusree";
 m.age = 25;
 m.salary = 80,000;
 m.pos = "Software Developer";

 m.showManager();
}
}

output:
Name: Tanusree
Age: 25
Salary: 80,000
Job Role: Software Developer
